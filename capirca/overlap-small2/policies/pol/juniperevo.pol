header {
  comment:: "Juniper EVO policy for overlap case."
  target:: juniperevo SamplePolicy inet ingress physical
}

term accept-web-services {
  source-address:: SRC_1
  destination-address:: SRC_2
  destination-port:: WEB_SERVICES
  protocol:: tcp
  action:: accept
}

term accept-ssh {
  source-address:: SRC_3
  destination-address:: SRC_4
  destination-port:: SSH
  protocol:: tcp
  action:: accept
}
